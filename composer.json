{
    "name": "jackalope/jackalope",
    "type": "library",
    "description": "Jackalope PHPCR library",
    "keywords": ["phpcr implementation"],
    "homepage": "http://jackalope.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jackalope Community",
            "homepage": "https://github.com/jackalope/jackalope/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "phpcr/phpcr-utils": "~1.0.0-beta6",
        "phpcr/phpcr": "=2.1.0-beta8",
        "ext-libxml": "*",
        "jackalope/jackalope-transport": "1.0.*"
    },
    "provide": {
        "phpcr/phpcr-implementation": "2.1.0-beta4"
    },
    "autoload": {
        "psr-0": { "Jackalope\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
